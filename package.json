{
  "name": "onboard-esdras-amora",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "yarn clean-dist && tsc && yarn copy-resources",
    "clean-dist": "rimraf ./dist || true",
    "copy-resources": "copyup 'src/**/*.{html,jpg,jpeg,png,css,json,wsdl,sql}' 'dist/'",
    "dev": "tsc-watch --onSuccess \"node -r ./tsconfig-paths-dist.js dist/index.js --inspect\" ",
    "exec": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "lint": "eslint --ext .js,.ts src",
    "prestart": "yarn lint && yarn build",
    "reinstall": "(rimraf node_modules || true) && yarn",
    "start": "node -r ./tsconfig-paths-dist.js dist/index.js"
  },
  "devDependencies": {
    "@types/express": "4.17.7",
    "@types/lodash": "^4.14.168",
    "@types/node": "14.6.0",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "9.0.0",
    "tsc-watch": "4.2.9",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "apollo-server-express": "2.19.1",
    "class-validator": "0.13.1",
    "dotenv": "8.2.0",
    "graphql": "15.3.0",
    "lodash": "^4.17.21",
    "pg": "8.5.1",
    "reflect-metadata": "0.1.13",
    "tsconfig-paths": "3.9.0",
    "type-graphql": "1.0.0",
    "typedi": "0.8.0",
    "typeorm": "0.2.25",
    "typeorm-typedi-extensions": "0.4.1",
    "uuid": "8.3.0"
  },
  "repository": "https://github.com/indigotech/onboard-esdras-amora.git",
  "author": "EsdrasAmora <esdras.amora@gmail.com>",
  "license": "MIT"
}
